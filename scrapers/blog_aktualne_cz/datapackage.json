            "description": "Number of readers"
          },
          {
            "name": "date",
            "type": "datetime",
            "description": "Published date"
          },
          {
            "name": "rank",
            "type": "integer",
            "description": "Current rank on the blogs' page"
          },
          {
            "name": "link",
            "type": "string",
            "description": "Blogpost link"
          },
          {
            "name": "scraped",
            "type": "datetime",
            "description": "Date and time of scraping"
          }
        ]
      }
